	message(STATUS "\t - @PROJECT_NAME@_LOCATION: ${@PROJECT_NAME@_LOCATION}") 
	message(STATUS "\t - @PROJECT_NAME@_INCLUDE_DIRS: ") 
	foreach(onePath ${@PROJECT_NAME@_INCLUDE_DIRS}) 
 		message(STATUS "\t\t - ${onePath}") 
	endforeach(onePath)
  
	message(STATUS "@PROJECT_NAME@ package done...\n") 

